package capa2;



// TODO: Auto-generated Javadoc

/**
 * The Interface Activitat.
 */
public interface Activitat {
	
	/**
	 * Gets the inici.
	 * 
	 * @return the temps
	 */
	abstract Temps GetInici();
	
	/**
	 * Sets the inici.
	 * 
	 * @param t the t
	 */
	abstract void SetInici(Temps t);
	
	/**
	 * Gets the duracio.
	 * 
	 * @return the int
	 */
	abstract int GetDuracio();

	/**
	 * Gets the tipus.
	 * 
	 * @return the int
	 */
	abstract int GetTipus();
	/*
	 * 1 - Descans
	 * 
	 * 2 - Comunicacio
	 */
	
	
	/**
	 * Sets the sessio.
	 * 
	 * @param s the s
	 */
	abstract void SetSessio(Sessio s);
	
	/**
	 * Gets the sessio.
	 * 
	 * @return the sessio
	 */
	abstract Sessio GetSessio();
	
	/**
	 * To string.
	 * 
	 * @return the string
	 */
	abstract String toString();
	
}
